{
  "types": {
    "vertex.string": {
      "class": "java.lang.String",
      "validateFunctions": [
        {
          "function": {
            "class": "gaffer.function.simple.filter.Exists"
          }
        }
      ]
    },
    "false": {
      "class": "java.lang.Boolean",
      "validateFunctions": [
        {
          "function": {
            "class": "gaffer.function.simple.filter.IsFalse"
          }
        }
      ]
    },
    "count.int": {
      "class": "java.lang.Integer",
      "validateFunctions": [
        {
          "function": {
            "class": "gaffer.function.simple.filter.IsMoreThan",
            "orEqualTo": true,
            "value": 0
          }
        }
      ]
    }
  }
}